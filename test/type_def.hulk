function test(x:Number, y:Number) => x + y;

protocol A{
    test(x:Object, x:Object): Object;
}

type Animal inherits A{
    self = 5;
    test(x: Number, x){
        let self = 0 in self := x;
    }
}
type Perro inherits Animal{
    test() => x - y;
    test() => x;
}

function test2(x, y){
    x + y;
    x - y;
}

type Point(x, y) {
    x: Yo = x;
    y = y;
}
type PolarPoint(phi:Number, rho:Number) inherits Point(rho * sin(phi), rho * cos(phi)) {
    phi = phi;
    rho = rho;
}

print(test(5, 5) + test2(4, 5));